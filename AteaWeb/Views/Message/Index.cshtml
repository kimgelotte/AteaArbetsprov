@model IEnumerable<AteaWeb.Models.MessageEntity>

@{
    ViewBag.Title = "Index";
}

<h2>Message</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MessageText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MessageText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageTime)
        </td>
    </tr>
}

</table>
